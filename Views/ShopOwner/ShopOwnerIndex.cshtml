<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Shopping Mall Management</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <style>
    #overtop {
    background-color: darkgoldenrod;
    height: 12px;
    }
    </style>
</head>
<body>
    @if (Session["userid"] != null)
    {
        @Html.Partial("shopownernav")

        <div class="container body-content">



            <h2 style="color:navy; text-align:center">Welcome to Shop Owner Page!</h2>
            <hr />
            <div style="color: black;border:double; align-content:center; font-size: 18px;">
                <ul>
                    <li>
                        You can update your profile.
                    </li>

                    <li>
                        You can view stores.
                    </li>
                    <li>
                        You can raise request for rent/lease.
                    </li>
                    <li>
                        You can view your request..
                    </li>
                </ul>
            </div>





        </div>

        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
    }
    else
    {
        @Html.Partial("sessionexpired");
    }
</body>
</html>
